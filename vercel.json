{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@4.0.0",
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/$", "dest": "/api/index.js" },
    { "src": "/start-workflow", "dest": "/api/start-workflow.js" },
    { "src": "/create-design", "dest": "/api/create_design.js" }
  ]
}
